{
  "name": "web-framework",
  "version": "0.1.0",
  "description": "Typescript web framework",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "lint": "tslint --force --format verbose \"src/**/*.ts\"",
    "build": "npm run clean && npm run lint && echo Using Typescript && tsc --version && tsc --pretty",
    "test": "npm run build && mocha --compilers ts:ts-node/register --recursive test/**/*-spec.ts",
    "watch": "npm run build -- --watch",
    "watch:test": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robspassky/web-framework.git"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "typescript",
    "node"
  ],
  "author": "Rob Cabacungan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/robspassky/web-framework/issues"
  },
  "homepage": "https://github.com/robspassky/web-framework#readme",
  "dependencies": {
    "@types/chai": "^3.4.34",
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.41",
    "chai": "^3.5.0",
    "mocha": "^3.1.0",
    "rimraf": "^2.5.4",
    "ts-node": "^1.3.0",
    "tslint": "^3.15.1",
    "typescript": "^2.0.3"
  },
  "devDependencies": {
    "@types/sinon": "^1.16.31",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.6",
    "tslint": "^3.15.1",
    "typescript": "^2.0.3"
  }
}
